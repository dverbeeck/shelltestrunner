# simple regexp
$$$  echo test
>>> /^test$/

# negative regexp
$$$  echo test
>>> !/toast/

# large regexp
# pcre-light handles regexps up to about 32764 chars, any larger gives a runtime error
#>>> 
# regex-tdfa memory usage blows up.. I fear to use anything more than 250 chars
$$$  ghc -ignore-dot-ghci -e "putStr $ replicate 33000 'a'"
>>> /aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa/

# regexp containing quoted /
$$$  echo 2015/1/1
>>> /2015\/1\/1/
